processData() - processAndSaveData()
// добавлено сохранение данных в название процедуры,
// так как в конце производится сохранение в файл,
// до этого без просмотра кода функции это было
// неявно

sendEmail() - sendEmailWithAttachment()
// изменено имя на отправку письма со вложением

loadConfiguration() - loadConfigurationFromFile()
// разделена логика по сохранению значений конфигурационных
// переменных из файла и из переменных окружения
// (loadConfigurationFromEnv)

generateReport() - generateAndEmailReport()
// отдельная процедура по генерации и отправке отчета по
// почте

updateRecord() - updateRecordAndNotify()
// уточнение в наименовании, что происходит отправка
// уведомления по обновлению записи

readFile() - readFileAndDecrypt()
// так как внутри происходит расшифровка файла перед
// его считыванием, это было добавлено в названии

encryptData() - encryptAndPrintData()
// перед записью в файл данные зашифровываются,
// что было отражено в названии

parseData() - parseAndValidateData()
// добавлена валидация в названии функции,
// так как это происходит после парсинга
// основных объектов, без просмотра кода
// это было непонятно

registerUser() - registerAndVerifyUser()
// в функции имеется блок кода верификации
// пользователя во время регистрации, это было
// учтено в названии функции 

initializeSystem() - initializeAndConfigureSystem()
// по запуску сервиса помимо инициализации читается
// конфигурационный файл и в последующем устанавливаются
// параметры запуска, что и было отражено в названии

cleanUpLogs() - archieveAndCleanUpLogs()
// функция в своей логике архивирует и перемещает
// старые логи в место хранения, а оставшиеся
// в основной директории удаляет. Архивирование
// не было указано, что и было добавлено в названии

backupDatabase() - сompressAndBackupDatabase()
// Embedded H2 база данных по истечению установленного
// времени сжималась и сохранялась в отдельном хранилище,
// это было учтено в названии
