ld - ldapConnection
// объект структуры хранения данных подключения к LDAP

rc - ldapReturnCode
// целое для хранения результата работы функций LDAP

dn - distinguishedName
// уникальное имя атрибута в каталоге LDAP

pwd - decodedPassword
// декодированное значение пароля пользователя из каталога LDAP

buffer - messageRequestBuffer
// массив символов полученного HTTP запроса от пользователя

rcvd - bytesReceived
// количество байтов, полученных от пользователя

needAuth - requiresAuthentication
// флаг определяющий нужна ли аутентификация

addr - clientAddress
// адрес клиентского сокета

st - fileStats
// объект структуры хранения данных о файле в ОС

stale - isAuthStale
// файл, указывающий, устарела ли проверка подлинности аутентификации

digest - digestContext
// объект структуры хранения сведений о текущем заголовке Digest-авторизации

p - addressInfoPointer
// указатель на текущую адресную информацию о сокете в связанном списке
